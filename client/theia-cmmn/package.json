{
    "name": "theia-cmmn",
    "description": "Glue code to integrate CMMN GLSP sprotty client into Eclipse Theia",
    "keywords": [
      "theia-extension"
    ],
    "author": {
      "name": "Simon Graband"
    },
    "license": "(EPL-2.0 OR GPL-2.0 WITH Classpath-exception-2.0)",
    "version": "0.2.0",
    "files": [
      "lib",
      "src"
    ],
    "homepage": "https://github.com/simongraband/cmmn-editor.git",
    "bugs": "https://github.com/simongraband/cmmn-editor.git",
    "repository": {
      "type": "git",
      "url": "https://github.com/simongraband/cmmn-editor.git"
    },
    "dependencies": {
      "@eclipse-glsp/theia-integration": "next",
      "@theia/core": "latest",
      "sprotty-cmmn": "0.2.0"
    },
    "devDependencies": {
      "rimraf": "latest",
      "typescript": "~3.5.3"
    },
    "scripts": {
      "prepare": "yarn run clean && yarn run build",
      "clean": "rimraf lib",
      "build": "tsc && yarn run lint",
      "lint": "tslint -c ../configs/tslint.json --project ./tsconfig.json",
      "watch": "tsc -w"
    },
    "theiaExtensions": [
      {
        "frontend": "lib/browser/frontend-extension",
        "backend": "lib/node/backend-extension"
      }
    ]
  }